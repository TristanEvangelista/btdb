import java.util.*;
import java.io.*;

public class btdb{
	RandomAccessFile file;
	long cntRecords;
	
	public static void main(String args[]) throws IOException{
    	Scanner in = new Scanner(System.in);
		String bt = args[0];
		String values = args[1];
		btdb mainBtdb = new btdb();
		RandomAccessFile fileBt = new RandomAccessFile(bt, "rwd");
		mainBtdb.ValuesRecords(values);
		
		while(true){
			String command = in.nextLine();
			String[] commandParts = command.split("\\s");
			if(commandParts[0].equals("insert") || commandParts[0].equals("update") || commandParts[0].equals("select") || commandParts[0].equals("exit")){
				if(commandParts[0].equals("insert")){
					int insertKey = Integer.parseInt(commandParts[1]);
					System.out.println(insertKey + " inserted.");
				}
				if(commandParts[0].equals("update")){
				
				}
				
				if(commandParts[0].equals("select")){
			
				}
				
				if(commandParts[0].equals("exit")){
					break;
				}
			}else{
				System.out.println("ERROR: invalid command");
			}
		}
		
		
	}
	
	public void ValuesRecords(String strFile) throws IOException{
		File file = new File(strFile);
		if(!file.exists()){
			this.cntRecords = 0;
			this.file = new RandomAccessFile(strFile, "rwd");
			this.file.seek(0);
			this.file.writeLong(this.cntRecords);
		}else{
			this.file = new RandomAccessFile(strFile,"rwd");
			this.file.seek(0);
			this.cntRecords = this.file.readLong();
			System.out.println(this.cntRecords);
		}
	}
}
